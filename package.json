{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "homepage": "https://USERNAME.github.io/REPO-NAME",
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server && npm run copy:htaccess",
    "build:client": "vite build",
    "build:gh-pages": "cross-env GITHUB_PAGES_BASE=/ npm run build:client && npm run copy:gh-pages",
    "copy:gh-pages": "npm run copy:htaccess",
    "build:server": "vite build --config vite.config.server.ts",
    "copy:htaccess": "node -e \"const fs=require('fs');const path=require('path');const dist=path.join(process.cwd(),'dist/spa');const files=['.htaccess','404.html','.nojekyll'];files.forEach(f=>{const src=path.join(process.cwd(),'client/public',f);const dest=path.join(dist,f);if(fs.existsSync(src)){fs.copyFileSync(src,dest);console.log('✅ Copied',f,'to dist/spa');}else if(f==='404.html'&&!fs.existsSync(dest)){const content='<!DOCTYPE html><html><head><meta charset=\\'UTF-8\\'><script>var l=window.location;l.replace(l.protocol+\\'//\\'+l.hostname+(l.port?\\':\\'+l.port:\\'')+l.pathname.split(\\'/\\').slice(0,1).join(\\'/\\')+\\'/index.html\\'+l.pathname.slice(1)+l.search+l.hash);</script></head><body><p>Redirecting...</p></body></html>';fs.writeFileSync(dest,content);console.log('✅ Created 404.html');}else if(f==='.nojekyll'&&!fs.existsSync(dest)){fs.writeFileSync(dest,'');console.log('✅ Created .nojekyll');}});\"",
    "start": "node dist/server/node-build.mjs",
    "start:prod": "NODE_ENV=production PORT=3000 node dist/server/node-build.mjs",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.2.0",
    "@stripe/stripe-js": "^7.9.0",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.3",
    "react-i18next": "^16.5.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
